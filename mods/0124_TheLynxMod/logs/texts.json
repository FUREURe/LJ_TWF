      "Npc_SmallOasis008_F_010": {
        "attributes": "Npc_SmallOasis008",
        "contents": [
          {
            "control": {
              "kind": "sound",
              "unknown": [
                1,
                23
              ]
            }
          },
          {
            "text": "That being said... You look purrfect!\nWith those clothes on, anyone\nis sure to see "
          },
          {
            "control": {
