        ]
      },
      "talk18": {
        "attributes": "Npc_SmallOasis008",
        "contents": [
          {
            "control": {
              "kind": "sound",
              "unknown": [
                7,
                8
              ]
            }
          },
          {
            "text": "Tu es tellement adorable..."
